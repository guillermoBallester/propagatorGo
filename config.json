{
  "app": {
    "name": "PropagatorGo",
    "version": "1.0.0",
    "port": 8081,
    "logLevel": "info",
    "apiPrefix": "/propagatorGo/v1",
    "env": "development"
  },
  "scraper": {
    "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/109.0.0.0 Safari/537.36",
    "maxDepth": 2,
    "maxRetries": 3,
    "randomDelay": 5000000000,
    "parallelLimit": 2,
    "sites": [
      {
        "name": "yahoo",
        "url": "https://finance.yahoo.com/quote/&1/news/",
        "allowedDomains": ["finance.yahoo.com", "www.finance.yahoo.com"],
        "articleContainerPath": "ul",
        "titlePath": "h3",
        "linkPath": "a",
        "textPath": "p",
        "enabled": true
      }
    ]
  },
  "scheduler": {
    "defaultTimeout": 300000000000,
    "jobs": [
      {
        "name": "news-scraper",
        "cronExpr": "0 */5 * * * *",
        "timeout": 180000000000,
        "retryCount": 2,
        "enabled": true,
        "description": "Scrapes news articles from configured sites"
      }
    ]
  },
  "database": {
    "host": "postgres",
    "port": 5432,
    "username": "postgres",
    "password": "postgres",
    "database": "propagatorGo",
    "sslMode": "disable"
  },
  "redis": {
    "address": "localhost:6379",
    "password": ""
  },
  "stockList": {
    "stocks": [
      {
        "symbol": "AAPL",
        "name": "Apple Inc.",
        "enabled": true
      },
      {
        "symbol": "MSFT",
        "name": "Microsoft Corporation",
        "enabled": true
      },
      {
        "symbol": "GOOGL",
        "name": "Alphabet Inc.",
        "enabled": true
      },
      {
        "symbol": "AMZN",
        "name": "Amazon.com, Inc.",
        "enabled": true
      },
      {
        "symbol": "META",
        "name": "Meta Platforms, Inc.",
        "enabled": true
      },
      {
        "symbol": "TSLA",
        "name": "Tesla, Inc.",
        "enabled": true
      },
      {
        "symbol": "NVDA",
        "name": "NVIDIA Corporation",
        "enabled": true
      },
      {
        "symbol": "NFLX",
        "name": "Netflix, Inc.",
        "enabled": true
      },
      {
